{
  "package": {
    "name": "com.intentfilter:android-permissions",
    "subject": "nishkarsh",
    "repo": "maven",
    "website_url": "https://github.com/nishkarsh/android-permissions",
    "issue_tracker_url": "https://github.com/nishkarsh/android-permissions/issues",
    "vcs_url": "https://github.com/nishkarsh/android-permissions",
    "licenses": [
      "MIT"
    ],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "{{VERSION_NUMBER}}",
    "gpgSign": false
  },
  "files": [
    {
      "includePattern": "build/outputs/aar/android-permissions-release.aar",
      "uploadPattern": "{{VERSION_NUMBER}}/android-permissions-{{VERSION_NUMBER}}.aar"
    },
    {
      "includePattern": "build/publications/androidPermissions/pom-default.xml",
      "uploadPattern": "{{VERSION_NUMBER}}/android-permissions-{{VERSION_NUMBER}}.pom"
    }
  ],
  "publish": false
}